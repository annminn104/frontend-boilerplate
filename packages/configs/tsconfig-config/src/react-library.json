{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React library with tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "esnext",
    "outDir": "dist",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "noEmit": false,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "composite": false
  }
}
