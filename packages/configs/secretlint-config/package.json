{
  "name": "@fe-boilerplate/secretlint-config",
  "version": "1.0.0",
  "description": "secretlint configuration for projects.",
  "keywords": [
    "secretlint",
    "secretlint-config"
  ],
  "homepage": "https://github.com/annminn104/frontend-boilerplate/tree/master/packages/configs/commitlint-config#readme",
  "bugs": {
    "url": "https://github.com/annminn104/frontend-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/annminn104/frontend-boilerplate.git",
    "directory": "packages/configs/secretlint-config"
  },
  "license": "ISC",
  "author": {
    "name": "Anh Minh (Matthew Nguyen)",
    "email": "annminn100@gmail.com",
    "url": "https://github.com/annminn104"
  },
  "main": "src/base.json",
  "files": [
    "src/base.json"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "format": "eslint --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix --cache --cache-location ../../.cache/eslint/secretlint-config.eslintcache",
    "lint": "eslint --ext .ts,.js,.cjs,.mjs --cache --cache-location ../../.cache/eslint/eslint-config.eslintcache",
    "lint:dist": "es-check -v",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@secretlint/secretlint-rule-preset-recommend": "9.0.0"
  },
  "devDependencies": {},
  "peerDependencies": {
    "secretlint": ">= 6"
  },
  "publishConfig": {
    "access": "public"
  }
}
